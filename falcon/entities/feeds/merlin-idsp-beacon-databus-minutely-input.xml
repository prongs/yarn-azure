<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<feed name="merlin-idsp-beacon-databus-minutely-input" description="render log generated by event hub" xmlns="uri:falcon:feed:0.1">
    <partitions>
        <partition name="seconds"/>
    </partitions>
    <frequency>minutes(5)</frequency>
    <timezone>UTC</timezone>
    <late-arrival cut-off="hours(6)"/>
    <clusters>
        <cluster name="local" version="0">
            <validity start="2018-04-20T00:00Z" end="2099-11-20T00:00Z"/>
            <retention limit="days(5)" action="delete"/>
        </cluster>
    </clusters>
    <locations>
        <location type="data" path="/idsp-enrichment/render-enhanced/${YEAR}/${MONTH}/${DAY}/${HOUR}/${MINUTE}"/>
    </locations>
    <ACL owner="fetl" group="users" permission="0x755"/>
    <schema location="/databus/streams_local/rr/schema/" provider="protobuf"/>
</feed>